{
    "GroupName": "", 
    "PolicyName": "", 
    "PolicyDocument": ""
}

{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": "ec2:DescribeInstances",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "ec2:TerminateInstances",
			"Resource": "arn:aws:ec2:ap-southeast
			1:968336292411:instance/*",
			"Condition": {
				"StringEquals": {
				"ec2:ResourceTag/stack": "dev"
				}
			}
		}
	]
}